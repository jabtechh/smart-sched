{
  "id": "/",
  "name": "SmartSched - Classroom Scheduling",
  "short_name": "SmartSched",
  "description": "Smart classroom scheduling and management system for scheduling rooms and managing reservations",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "browser"],
  "orientation": "portrait-primary",
  "background_color": "#ffffff",
  "theme_color": "#297A20",
  "categories": ["productivity", "utilities"],
  "lang": "en-US",
  "dir": "ltr",
  "iarc_rating_id": "e84b9d8f-f8c6-4ebc-95ad-d36d65d79f2e",
  "icons": [
    {
      "src": "/icons/icon-192x192.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-512x512.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/icons/screenshot-narrow.svg",
      "sizes": "540x720",
      "type": "image/svg+xml",
      "form_factor": "narrow"
    },
    {
      "src": "/icons/screenshot-wide.svg",
      "sizes": "1280x720",
      "type": "image/svg+xml",
      "form_factor": "wide"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.pateros.smartsched"
    }
  ],
  "shortcuts": [
    {
      "name": "Scan Room",
      "short_name": "Scan",
      "description": "Scan QR code to check in to a room",
      "url": "/professor/scan",
      "icons": [
        {
          "src": "/icons/icon-192x192.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "View Reservations",
      "short_name": "Reservations",
      "description": "View and manage your reservations",
      "url": "/prof/reservations",
      "icons": [
        {
          "src": "/icons/icon-192x192.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "scope_extensions": [
    {
      "origin": "https://*.pateros.edu.ph"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+smartsched",
      "url": "/?qr=%s"
    }
  ],
  "launch_handler": {
    "client_mode": ["navigate-new", "auto"]
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "text/csv": [".csv"]
      },
      "icons": [
        {
          "src": "/icons/icon-192x192.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "widgets": [
    {
      "name": "SmartSched Schedule",
      "short_name": "Schedule",
      "description": "Quick access to your schedule",
      "tag": "smartsched-schedule",
      "screenshots": [
        {
          "src": "/icons/screenshot-narrow.svg",
          "sizes": "540x720",
          "type": "image/svg+xml",
          "form_factor": "narrow"
        }
      ],
      "data": "{}",
      "screenshots": []
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  }
}
